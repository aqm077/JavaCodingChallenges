class Solution {
    public void reverseString(char[] s) {
        int pointerA = 0;
        int pointerB = s.length - 1;

        while (pointerA < pointerB) {
            char temp = s[pointerA];
            s[pointerA] = s[pointerB];
            s[pointerB] = temp;

            pointerA++;
            pointerB--;
        }
    }
}